  "bin": {
    "nephele-serve": "nephele-serve.cjs"
  },
  "scripts": {
    "clean": "test -d bin && rm -r bin || true",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
