    "cli",
    "command line",
    "terminal",
    "command",
    "personal cloud",
    "local file server",
    "file share",
    "s3",
    "aws s3",
    "s3 file server",
    "object store",
    "object storage",
    "blob store",
    "blob storage",
    "nas",
    "network storage",
    "nephele"
