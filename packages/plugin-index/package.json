{
  "name": "@nephele/plugin-index",
  "version": "1.0.0-alpha.62",
  "description": "Index file and directory contents listing plugin (browser support) for the Nephele WebDAV server.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "npm run build:ts && npm run build:svelte",
    "build:ts": "tsc",
    "build:svelte": "node compileIndexPage.cjs",
    "watch": "tsc --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nephele.git"
  },
  "keywords": [
    "webdav",
    "nephele",
    "plugin",
    "nephele plugin",
    "directory listing",
    "directory index"
  ],
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/nephele/issues"
  },
  "homepage": "https://github.com/sciactive/nephele#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "nephele": "^1.0.0-alpha.62",
    "svelte": "^4.2.19"
  },
  "engines": {
    "node": ">=18"
  }
}
